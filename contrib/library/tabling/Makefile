CIAOCMD ?= ciao

clean: 
	$(CIAOCMD) clean_tree "."
	$(CIAOCMD) clean_tree "../difference_constraints"
	$(CIAOCMD) clean_tree "../disequalities"
	/bin/rm -f *~
	/bin/rm -f examples/*~
	/bin/rm -f examples/bench/*~
	/bin/rm -f Tests/*~
	/bin/rm -f Tests/bench/*~
	/bin/rm -f CCAT/*~
	/bin/rm -f ../difference_constraints/*~
	/bin/rm -f ../difference_constraints/difference_constraints_tab/*~
	/bin/rm -f ../difference_constraints/difference_constraints_tab/examples/*~
	/bin/rm -f ../disequalities/*~
	/bin/rm -f ../disequalities/disequalities_tab/*~
	/bin/rm -f ../disequalities/disequalities_tab/examples/*~
