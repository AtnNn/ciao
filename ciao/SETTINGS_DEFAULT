# -*- mode: Makefile; -*-
# Minimal settings file for compiling the engine for the first time

# Note: it is hightly recommended to use $(PWD) instead of $(shell
# pwd) because the latter resolves the soft links and that behavior is
# undesirable for our goals.

INSTYPE=src
SYSAVAIL=user
SILENT=true
USE_THREADS=yes
USE_POSIX_LOCKS=no
AND_PARALLEL_EXECUTION=no
PAR_BACK=no
TABLED_EXECUTION=no
OPTIM_LEVEL=optimized
COMPRESS_LIBS=no
EXECMODE=775
DATAMODE=664
DEBUG_LEVEL=install
INSTALLGROUP=
# Assume that svnversion is not available
HAVE_SVNVERSION=no
